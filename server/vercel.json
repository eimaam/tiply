{
  "version": 2,
  "name": "tiply-backend",
  "builds": [
    {
      "src": "dist/index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "dist/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
